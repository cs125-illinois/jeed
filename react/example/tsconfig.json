{
  "compilerOptions": {
    "noEmit": true,
    "target": "es2019",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "strict": true,
    "alwaysStrict": false,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "types": ["node"],
    "typeRoots": ["node_modules/@types", "./typings"],
    "baseUrl": ".",
    "resolveJsonModule": true,
    "paths": {
      "@cs125/jeed": [".."]
    }
  }
}
