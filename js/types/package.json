  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "checker": "yarn depcheck && yarn prettier && yarn eslint && yarn tsc",
    "clean": "rimraf dist",
    "depcheck": "depcheck --ignores=prettier-plugin-*",
    "eslint": "eslint src/**",
    "ncu": "ncu",
    "prepublish": "rimraf dist && tsc",
    "start": "yarn watch",
    "test": "env-cmd -f .env.test ts-mocha --paths tests/index.spec.ts",
    "tsc": "tsc --noEmit",
    "prettier": "prettier -w --plugin=prettier-plugin-organize-imports . && prettier-package-json --write",
    "watch": "tsc -w"
  },
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "runtypes": ">= 6"
  },
  "devDependencies": {
    "@types/deep-diff": "1.0.1",
    "@types/js-yaml": "4.0.5",
    "@types/lodash": "4.14.185",
    "@types/mocha": "9.1.1",
    "@types/node": "18.7.16",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "deep-diff": "1.0.2",
    "depcheck": "1.4.3",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "isomorphic-fetch": "3.0.0",
    "js-yaml": "4.1.0",
    "lodash": "4.17.21",
    "mocha": "10.0.0",
    "npm-check-updates": "16.1.2",
    "prettier": "2.7.1",
    "prettier-package-json": "2.6.4",
    "prettier-plugin-organize-imports": "3.1.1",
    "rimraf": "3.0.2",
    "runtypes": "6.6.0",
    "ts-mocha": "10.0.0",
    "typescript": "4.8.3"
  }
}
